
<%- include('./partials/admin-nav') %>


  <!-- Admin Panel Content -->
  <div class="container-fluid p-4">
    <%- include('./partials/messages') %>

    <div class="row">

      <!-- Total Contestants Card -->
      <div class="col-md-4 col-lg-2 mb-4 ">
        <div class="dash-height ">
            <h6 class="">Total Contestants</h5>
            <p class="card-text" id="totalContestants"><%=contestants.length%></p>
          </div>
      </div>

      <!-- Total Contestants Card -->
      <div class="col-md-4 col-lg-2 mb-4 ">
        <div class="dash-height bg-danger">
            <h6 class="">New Vote claims</h5>
            <p class="card-text" id="totalContestants"><%=votesClaims.length%></p>
          </div>
      </div>

      <!-- Total Contestants Card -->
      <div class="col-md-4 col-lg-2 mb-4 ">
        <div class=" dash-height">
            <h6 class="">Total claims</h6>
            <p class="card-text" id="totalContestants"><%=allVotes.length%></p>
          </div>
      </div>


    </div>


  </div>

  <!-- Bootstrap JS -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js"></script>
  <script>
    // Display current date
    document.getElementById('currentDate').innerText = new Date().toLocaleDateString();
  </script>
